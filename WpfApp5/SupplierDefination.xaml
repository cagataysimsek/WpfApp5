<Window x:Class="WpfApp5.SupplierDefination"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5"
        mc:Ignorable="d"
        Title="SupplierDefination" Height="250" Width="600">
    <Grid Loaded="Grid_Loaded">
        <ComboBox Name="comboSupplierName" Width="180" Height="20" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="262,75,0,0" local:ComboBoxBehavior.CharacterCasing="Upper" IsEditable="True"/>
        <Label Content="Tedarikçi Adı Giriniz:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="141,72,0,0" />
        <Button x:Name="buttonEkle" Content="Ekle" HorizontalAlignment="Left" Height="24" Margin="420,144,0,0" VerticalAlignment="Top" Width="93" Click="buttonEkle_Click"/>
        <Button x:Name="buttonSil" Content="Sil" HorizontalAlignment="Left" Height="24" Margin="121,144,0,0" VerticalAlignment="Top" Width="100" Click="buttonSil_Click"/>
        <Button Name="buttonGuncelle" Content="Güncelle" HorizontalAlignment="Left" Height="24" Margin="259,144,0,0" VerticalAlignment="Top" Width="100" Click="buttonGuncelle_Click"/>
    </Grid>
</Window>
